<!--This js represent 4 card dragging effect left to right to left-->


<script>
    document.addEventListener("DOMContentLoaded", function () {
        const inner = document.querySelector(".inner");
        const cards = document.querySelectorAll(".card");
        const totalCards = cards.length;
        let isStopped = false;

        cards.forEach((card, index) => {
            card.addEventListener("click", () => {
                // Rotation angle calculate karo
                const angle = 360 / totalCards;
                const rotateTo = -angle * index;

                // Animation rok do
                inner.style.animation = "none";
                isStopped = true;

                // Transform set karo
                inner.style.transform = `perspective(1000px) rotateX(-15deg) rotateY(${rotateTo}deg)`;
            });
        });

        inner.addEventListener("mouseleave", () => {
            if (isStopped) {
                // Mouse leave pe animation wapas start karo
                inner.style.animation = "rotating 10s linear infinite";
                isStopped = false;
            }
        });
    });


   //Sweet alert
