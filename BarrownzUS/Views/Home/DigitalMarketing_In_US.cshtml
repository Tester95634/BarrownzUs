<script>

    document.addEventListener("DOMContentLoaded", function () {
        const inner = document.querySelector(".inner");
        let isDragging = false;
        let startX, currentX, rotationY = 0;

        inner.addEventListener("mouseenter", () => {
            const currentTransform = computedStyle.transform;
            inner.style.transform = currentTransform;
            inner.style.animation = "none"; // Hover par animation stop
            const computedStyle = window.getComputedStyle(inner);
        });

        //inner.addEventListener("mouseenter", () => {
        //    const computedStyle = window.getComputedStyle(inner);
        //    const currentTransform = computedStyle.transform; // Current transform value lo
        //    inner.style.animation = "none"; // Animation stop karo
        //    inner.style.transform = currentTransform; // Current position freeze raho
        //});

        inner.addEventListener("mouseleave", () => {
            if (!isDragging) {
                inner.style.animation = "rotating 10s linear infinite"; // Mouse hatao to animation start
            }
        });

        inner.addEventListener("mousedown", (e) => {
            isDragging = true;
            startX = e.clientX;
            inner.style.animation = "none"; // Dragging par bhi animation stop
        });

        document.addEventListener("mousemove", (e) => {
            if (!isDragging) return;
            currentX = e.clientX;
            let deltaX = currentX - startX;
            rotationY += deltaX * 0.2; // Sensitivity control
            inner.style.transform = `perspective(1000px) rotateX(-15deg) rotateY(${rotationY}deg)`;
            startX = currentX;
        });

        document.addEventListener("mouseup", () => {
            isDragging = true;
            inner.style.animation = "rotating 10s linear infinite"; // Dragging chhodo to animation start
        });

        // Mobile touch support
        inner.addEventListener("touchstart", (e) => {
            isDragging = true;
            startX = e.touches[0].clientX;
            inner.style.animation = "none";
        });

        document.addEventListener("touchmove", (e) => {
            if (!isDragging) return;
            currentX = e.touches[0].clientX;
            let deltaX = currentX - startX;
            rotationY += deltaX * 0.2;
            inner.style.transform = `perspective(1000px) rotateX(-15deg) rotateY(${rotationY}deg)`;
            startX = currentX;
        });

        document.addEventListener("touchend", () => {
            isDragging = false;
            inner.style.animation = "rotating 10s linear infinite"; // Touch chhodo to animation start
        });
    });
</script>